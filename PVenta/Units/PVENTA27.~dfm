        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end
      item
        Name = 'num'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'select'
      'emp_codigo, ent_numero, pro_codigo,'
      'UnidadID, Precio, Cantidad, Secuencia, Credito,'
      'Minimo, suc_codigo'
      'from'
      'EntradaInvUnidadMedida'
      'where'
      'emp_codigo = :emp'
      'and suc_codigo = :suc'
      'and ent_numero = :num'
      'order by'
      'Secuencia')
    Left = 368
    Top = 208
    object QUnidadesemp_codigo: TIntegerField
      FieldName = 'emp_codigo'
    end
    object QUnidadesent_numero: TIntegerField
      FieldName = 'ent_numero'
