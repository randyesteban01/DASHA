  object QUpdDev: TADOQuery
    Connection = DM.ADOSigma
    Parameters = <
      item
        Name = 'NCF'
        DataType = ftString
        Size = -1
        Value = Null
      end
      item
        Name = 'emp'
        DataType = ftInteger
        Size = -1
        Value = Null
      end
      item
        Name = 'suc'
        DataType = ftInteger
        Size = -1
        Value = Null
      end
      item
        Name = 'num'
        DataType = ftInteger
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'update devolucion '
      'set NCF           =  :NCF,'
      '    NCF_Fijo      = b.NCF_Fijo,'
      '    NCF_Secuencia = b.NCF_Secuencia'
      'from devolucion a'
      'inner join notascredito b on a.emp_codigo = b.emp_codigo'
      '                         and a.suc_codigo = b.suc_codigo'
      #9'        and a.dev_numero = b.ncr_devolucion'
      'where a.emp_codigo = :emp '
      'and   a.suc_codigo    = :suc'
      'and   a.dev_numero = :num')
    Left = 224
    Top = 248
  end
