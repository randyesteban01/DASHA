  object QCheques: TADOQuery
    Connection = DM.ADOSigma
    Parameters = <
      item
        Name = 'sol1'
        DataType = ftInteger
        Size = -1
        Value = Null
      end
      item
        Name = 'sol2'
        DataType = ftInteger
        Size = -1
        Value = Null
      end
      item
        Name = 'emp'
        DataType = ftInteger
        Size = -1
        Value = Null
      end
      item
        Name = 'suc'
        DataType = ftInteger
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'select'
      'b.ban_nombre, c.che_numero, c.che_fecha, c.che_monto,'
      'c.ban_codigo'
      'from'
      'cheques c, bancos b'
      'where'
      'c.emp_codigo = b.emp_codigo'
      'and c.ban_codigo = b.ban_codigo'
      'and c.suc_codigo = b.suc_codigo'
      'and c.che_numero in (select sol_cheque'
      'from solicitud where emp_codigo = c.emp_codigo'
      'and sol_cheque = c.che_numero'
      'and ban_codigo = c.ban_codigo'
      'and sol_numero between :sol1 and :sol2)'
      'and c.emp_codigo = :emp'
      'and c.suc_codigo = :suc'
      'order by'
      'c.ban_codigo, c.che_numero')
    Left = 168
    Top = 312
    object QChequesban_nombre: TStringField
      FieldName = 'ban_nombre'
      Size = 60
    end
    object QChequesche_numero: TIntegerField
      FieldName = 'che_numero'
    end
    object QChequesche_fecha: TDateTimeField
      FieldName = 'che_fecha'
    end
